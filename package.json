{
  "type": "module",
  "scripts": {
    "test": "ava --verbose --timeout=20s",
    "tiles": "rm -rf AoCTiles/.aoc_tiles_cache && python3 AoCTiles/create_aoc_tiles.py",
    "compile": "tsc --outDir dist --noEmit"
  },
  "devDependencies": {
    "@swc/core": "^1.3.24",
    "ava": "^5.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "ava": {
    "files": [
      "./**/*test.ts"
    ],
    "nodeArguments": [
      "--experimental-specifier-resolution=node",
      "--loader=ts-node/esm",
      "--no-warnings"
    ],
    "extensions": {
      "ts": "module"
    }
  },
  "dependencies": {
    "circularray": "^0.2.2",
    "js-levenshtein": "^1.1.6"
  },
  "engines": {
    "node": "18"
  }
}
